<html>
    <head>
        <title>
            <%= title %>
        </title>
    </head>
    <body>
        <h1>Playground</h1>
        <ul>
            <!-- for loop -->
            <% for (let i=1;i<10;i++) { %>
            <li>
                <%= i %>
            </li>
            <%}%>
        </ul>
        <!-- condional -->
        <% if(true){ %>
           <h1>It's true</h1>
        <%}else{ %>
            <p>It's false</p>
        <%} %>
    </body>
</html>